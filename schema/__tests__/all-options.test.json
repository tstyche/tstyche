{
  "target": "../config.json",
  "tests": [
    {
      "description": "all options",
      "valid": true,
      "data": {
        "checkDeclarationFiles": true,
        "checkSuppressedErrors": true,
        "failFast": true,
        "fixtureFileMatch": ["fixtures/*.ts", "**/__fixtures__/**/*"],
        "quiet": true,
        "rejectAnyType": true,
        "rejectNeverType": true,
        "reporters": ["./tstyche-reporter.js"],
        "target": ">=5.2 <=5.3 || 5.4.2 || >5.5",
        "testFileMatch": ["examples/*.test.ts", "**/__typetests__/*.test.ts"],
        "tsconfig": "./tsconfig.test.json",
        "verbose": true
      }
    }
  ]
}
