{
  "cache": {
    "cacheLocation": "./.cache/cspell",
    "useCache": true
  },
  "files": [
    "**/*.js",
    "**/*.json",
    "**/*.md",
    "examples/**/*",
    "models/**/*",
    "source/**/*",
    "tests/**/*",
    "typetests/**/*"
  ],
  "ignorePaths": ["**/__snapshots__/**/*"],
  "language": "en-US",
  "useGitignore": true,
  "words": ["codacy", "findup", "identicality", "jsxs", "monocart", "nodenext", "tsserverlibrary", "typetests"]
}
