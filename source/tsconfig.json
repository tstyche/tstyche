{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "paths": {
      "#*": ["./*/index.ts"],
      "#scribbler/jsx-runtime": ["./scribbler/Scribbler.ts"]
    },

    "jsx": "react-jsx",
    "jsxImportSource": "#scribbler",

    "skipLibCheck": false
  },
  "include": ["**/*"],
  "exclude": ["**/__tests__/**/*"]
}
